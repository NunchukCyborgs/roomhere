{
  "name": "roomhere",
  "version": "0.9.0",
  "description": "Roomhere",
  "repository": {
    "type": "git",
    "url": "https://github.com/NunchukCyborgs/roomhere.git"
  },
  "scripts": {
    "restart": "node node_modules/rimraf/bin.js node_modules && npm cache clean && npm i && npm start",
    "build:scss": "node node_modules/node-sass/bin/node-sass src/app -r -o src/app",
    "watch:scss": "node node_modules/node-sass/bin/node-sass -w src/app -r -o src/app",
    "ballz": "forever start --killSignal=SIGTERM node_modules/nodemon/bin/nodemon.js",

    "watch": "node node_modules/webpack/bin/webpack.js --watch",
    "watch:dev": "npm run server & npm run watch",
    "clean:dist": "node node_modules/rimraf/bin.js dist",
    "clean:css": "node node_modules/rimraf/bin.js **/styles.css",
    "clean:ngc": "node node_modules/rimraf/bin.js **/*.ngfactory.ts",
    "clean": "npm run clean:dist && npm run clean:ngc && npm run clean:css",
    "prebuild": "npm run clean && npm run build:scss",
    "build": "node node_modules/webpack/bin/webpack.js  --progress",
    "build:prod:ngc": "npm run prebuild && npm run ngc && npm run build:prod",
    "build:prod:ngc:json": "npm run prebuild && npm run ngc && npm run build:prod:json",
    "build:prod": "node node_modules/webpack/bin/webpack.js --config webpack.prod.config.ts",
    "build:prod:json": "node node_modules/webpack/bin/webpack.js --config webpack.prod.config.ts --json | webpack-bundle-size-analyzer",
    "ngc": "npm run ngc:browser && npm run ngc:node",
    "ngc:browser": "ngc -p tsconfig.browser.json",
    "ngc:node": "ngc -p tsconfig.node.json",
    "prestart": "npm run build",
    "server": "nodemon dist/server/index.js",
    "debug:server": "node-nightly --inspect --debug-brk dist/server/index.js",
    "start": "npm run server",
    "debug:start": "npm run build && npm run debug:server",
    "predebug": "npm run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug:build:prod": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js  --config webpack.prod.config.ts",
    "debug": "node --debug-brk dist/server/index.js"
  },
  "license": "MIT",
  "contributors": [
    "Josiah Burchard <josiah@roomhere.io>",
    "Joe Edgar <joe@roomhere.io>",
    "Zach Young <zach@roomhere.io>",
    "Stephen Myers <stephen@roomhere.io>"
  ],
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@angular/router": "3.2.1",
    "@angularclass/resolve-angular-routes": "1.0.9",
    "@types/body-parser": "^0.0.29",
    "@types/compression": "^0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/hammerjs": "^2.0.32",
    "@types/mime": "0.0.28",
    "@types/node": "^6.0.38",
    "@types/serve-static": "^1.7.27",
    "@ngtools/webpack": "^1.1.4",
    "angular2-express-engine": "2.1.0-rc.1",
    "angular2-platform-node": "2.1.0-rc.1",
    "angular2-template-loader": "0.6.0",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "autoprefixer": "6.5.3",
    "awesome-typescript-loader": "2.2.4",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "css-loader": "0.26.0",
    "express": "4.14.0",
    "js.clone": "0.0.3",
    "modern-lru": "1.0.8",
    "express-minify-html": "0.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "foundation-sites": "6.2.3",
    "honeybadger": "1.2.0",
    "html-loader": "0.4.4",
    "methods": "1.1.2",
    "motion-ui": "1.2.2",
    "node-sass": "3.13.0",
    "postcss-loader": "1.1.1",
    "progressbar.js": "1.0.1",
    "purifycss-webpack-plugin": "2.0.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.2",
    "serve-favicon": "2.3.2",
    "ts-node": "1.7.0",
    "typescript": "2.0.3",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.0.0",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-merge": "0.16.0",
    "zone.js": "0.6.26",
    "preboot": "4.5.2",
    "imports-loader": "^0.6.5",
    "string-replace-loader": "^1.0.5",
    "ts-helpers": "^1.1.2",
    "v8-lazy-parse-webpack-plugin": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
}
