{
  "name": "roomhere",
  "version": "0.9.0",
  "description": "Roomhere",
  "repository": {
    "type": "git",
    "url": "https://github.com/NunchukCyborgs/roomhere.git"
  },
  "scripts": {
    "watch": "node node_modules/webpack/bin/webpack.js --watch",
    "prebuild": "node node_modules/rimraf/bin.js dist",
    "build": "node node_modules/webpack/bin/webpack.js",
    "build:prod": "node node_modules/webpack/bin/webpack.js --progress -p --optimize-minimize",
    "prestart": "npm run build",
    "server": "nodemon dist/server/index.js",
    "start": "npm run server",
    "predebug": "npm run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug": "node --debug-brk dist/server/index.js",
    "restart": "node node_modules/rimraf/bin.js node_modules && npm cache clean && npm i && npm start",
    "ballz": "forever start --killSignal=SIGTERM node_modules/nodemon/bin/nodemon.js"
  },
  "license": "MIT",
  "contributors": [
    "Josiah Burchard <josiah@roomhere.io>",
    "Joe Edgar <joe@roomhere.io>",
    "Zach Young <zach@roomhere.io>",
    "Stephen Myers <stephen@roomhere.io>"
  ],
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "@angularclass/resolve-angular-routes": "1.0.9",
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "1.3.30",
    "@types/express": "4.0.33",
    "@types/express-serve-static-core": "4.0.36",
    "@types/mime": "0.0.29",
    "@types/node": "6.0.41",
    "@types/serve-static": "1.7.31",
    "angular2-express-engine": "2.0.11",
    "angular2-platform-node": "2.0.11",
    "angular2-template-loader": "0.5.0",
    "angular2-universal": "2.0.11",
    "angular2-universal-polyfills": "2.0.11",
    "autoprefixer": "6.5.0",
    "awesome-typescript-loader": "2.2.4",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "express-minify-html": "0.6.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "foundation-sites": "6.2.3",
    "honeybadger": "1.1.3",
    "html-loader": "0.4.4",
    "json-loader": "0.5.4",
    "methods": "1.1.2",
    "motion-ui": "1.2.2",
    "node-sass": "3.10.1",
    "postcss-loader": "0.13.0",
    "progressbar.js": "1.0.1",
    "purifycss-webpack-plugin": "2.0.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.2",
    "serve-favicon": "2.3.0",
    "ts-node": "1.3.0",
    "typescript": "2.0.3",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.22",
